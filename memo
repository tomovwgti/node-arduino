curl http://localhost:3000/status?status=g

after build

RESULT=`curl http://localhost:9090/jenkins/job/SilkBrowser/lastBuild/api/xml | perl -le '$_=<>;print [/<result>(.+?)</]->[0]'`

echo $RESULT

if [ $RESULT = SUCCESS ]
then
    echo "success"
    curl http://localhost:3000/status?status=s
else
    echo "failure"
    curl http://localhost:3000/status?status=e
fi
